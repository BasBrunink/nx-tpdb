    "typeorm:generate-migration": "npm run typeorm -- -d  migration:generate ./apps/tpdb-backend-mono/migrations/structure/$npm_config_name",
    "typeorm:create-migration:dummy": "npm run typeorm -- migration:create ./apps/tpdb-backend-mono/migrations/dummy/$npm_config_name",
    "typeorm:create-migration:seed": "npm run typeorm -- migration:create ./apps/tpdb-backend-mono/migrations/seed/$npm_config_name",
    "typeorm:create-migration:structure": "npm run typeorm -- migration:create ./apps/tpdb-backend-mono/migrations/structure/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/config/typeOrm.config.ts migration:revert",
    "extract-translations": "ngx-translate-extract --input ./apps/tpdb-admin-portal/src --output ./apps/tpdb-admin-portal/public/assets/i18n/*.json --sort --format namespaced-json --marker _"
